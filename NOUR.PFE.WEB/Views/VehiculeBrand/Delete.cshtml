@model NOUR.PFE.Entities.VehiculeBrand

@{
    Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";

}

<section class="content-header" style="width:600px; margin-left:230px; padding-top:150px">
    <div class="container-fluid">

        <div class="card  card-teal card-outline">
            <div class="card-header">

                <h3>Are you sure you want to delete this?</h3>
            </div>

            <div class="card-body">

                <div class="row">
                    <div class="col-md-12">
                        <form asp-action="Delete">
                            <a class="float-right">
                                <input type="submit" value="Delete" class="btn btn-danger" />
                            </a>
                            <input asp-action="Index" type="submit" value="Back to List" class="btn btn-primary" />
                        </form>
                    </div>
                </div>

            </div>
        </div>
    </div>
</section>





